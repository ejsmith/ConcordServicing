{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "OpenTelemetry": {
        "ServiceName": "concord",
        "EnableLogs": true,
        "EnableTracing": true,
        "SampleRate": 1.0,
        "FullDetails": true,
        "Debug": false
    }
}
